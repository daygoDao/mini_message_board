<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% for(let message of messages) { %>
        <li>
          <section>
            <h4><%= message.user %></h4>
            <p><%= message.text %></p>
            <p><%= message.add %></p>
          </section>
        </li>
        <% } %>
    </ul>
  </body>
</html>
